  flex-wrap wrap
  > *
    width 25%
.up-col-5
  display flex
  flex-wrap wrap
  > *
    width 20%
.up-col-6
  display flex
  flex-wrap wrap
  > *
    width 16.666%

/* gapy(px) */
for gapy-px in range(0,100,10)
  .up-gapy-{gapy-px}
    > *
      padding-bottom gapy-px px
/* gapy(%) */
for gapy in range(1,10,1)
  .up-gapy-{gapy}\\\%
    > *
      padding-bottom (gapy)%

/* gapx(px) */
for gapx-px in range(0,100,10)
  .up-gapx-{gapx-px}
    margin-left -(gapx-px/2)px
    margin-right -(gapx-px/2)px
    > *
      padding-left (ceil((gapx-px/2),3))px
      padding-right (ceil((gapx-px/2),3))px
/* gapx(%) */
for gapx in range(1,10,1)
  .up-gapx-{gapx}\\\%
    margin-left -(gapx/2)%
    margin-right -(gapx/2)%
    > *
      padding-left (ceil((gapx/2),3))%
      padding-right (ceil((gapx/2),3))%


/* 画像比率固定 */
for image in range(10,170,5)
  .up-ofi-{image}
    &:after
      content ""
      display block
      padding-top (image)%
    img
      position absolute
      top 0
      left 0

/* object-position */
.up-obp-c img
  font-family "object-fit: cover; object-position: center center;"
  object-position center
.up-obp-t img
  font-family "object-fit: cover; object-position: center top;"
  object-position top
.up-obp-r img
  font-family "object-fit: cover; object-position: right center;"
  object-position right
.up-obp-b img
  font-family "object-fit: cover; object-position: center bottom;"
  object-position bottom
.up-obp-l img
  font-family "object-fit: cover; object-position: left center;"
  object-position left

/* object-position */
.up-obf-contain img
  font-family "object-fit: contain; object-position: center center;"
  object-fit contain


/* 表示・非表示 */
.up-show
