  overflow hidden
  &:after
    display block
    padding-top 50%
    content ""
  iframe
    position absolute
    top 0
    right 0
    left 0
    margin auto
    max-width initial
    width 100%
    height 100%

a,
button
  color inherit
  text-decoration none
  transition 0.4s ease
  &:hover
    opacity 0.6
    text-decoration none
    color inherit
    cursor pointer
